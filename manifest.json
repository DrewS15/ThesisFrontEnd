{
    "manifest_version":3,
    "name": "Hate Speech Detection",
    "description": "Hate Speech Analysis Extension",
    "version": "0.3.2",
    "action": {
        "default_popup": "HSD.html",
        "default_icon": "hello_extensions.png"
    },
    "sandbox":{
        "pages":[
            "HSD.html"
        ]
    },
    
    "content_security_policy" : {
        "sandbox": "sandbox allow-scripts; script-src 'unsafe-eval' 'self' https://pyscript.net https://cdn.jsdelivr.net"
    }
}  
    