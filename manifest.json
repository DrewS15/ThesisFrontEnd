{
    "name": "Filipino Hatespeech Detectionv3 (git)",
    "description": "official filipino hatespeech detection demo",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "userInterface.html",
        "default_icon": "images/32x32_apple_texture.png"
    },
    "background": {
        "service_worker": "contextmenu.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["cloudservice.js"]
        }
    ],
    "permissions":[
      "contextMenus",
      "tabs",
      "storage",
      "scripting",
      "activeTab",
      "notifications"
    ]
}