{
    "name": "Filipino Hatespeech Detectionv3",
    "description": "official filipino hatespeech detection demo",
    "version": "1.0",
    "manifest_version": 3,
    "action": {"default_popup": "popup.html"},
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["popup.js"]
        }
    ],
    "permissions":[
      "contextMenus",
      "tabs",
      "storage",
      "scripting",
      "activeTab"
    ]
}