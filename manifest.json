{
    "name": "Filipino Hatespeech Detection",
    "description": "official filipino hatespeech detection demo",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "userInterface.html"
    },
    "icons": {
        "128": "images/128x128_senty.png"
    },
    "background": {
        "service_worker": "contextmenu.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["functions.js"]
        }
    ],
    "permissions":[
      "contextMenus",
      "tabs",
      "storage",
      "scripting",
      "activeTab",
      "notifications"
    ]
}